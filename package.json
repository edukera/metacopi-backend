{
  "name": "metacopi-backend",
  "version": "1.0.0",
  "description": "backend for Metacopi App",
  "author": "edukera",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=512 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --config=jest.config.js",
    "test:exit": "jest --forceExit --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:cov": "jest --coverage --config=jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config=jest.config.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "seed": "ts-node src/seed.ts",
    "seed:local": "node scripts/seed-local.js",
    "seed:prod": "node scripts/seed-prod.js",
    "dump:prod": "node scripts/dump-prod.js",
    "dump:local": "node scripts/dump-local.js",
    "dump": "ts-node src/dump.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.0.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.20",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.20",
    "@nestjs/swagger": "^11.1.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.0",
    "fuse.js": "^7.0.0",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0",
    "mongoose": "^8.13.2",
    "nest-winston": "^1.10.2",
    "openai": "^4.89.0",
    "ora": "^8.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@golevelup/ts-jest": "^0.6.2",
    "@nestjs/testing": "^11.0.20",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
